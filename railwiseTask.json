{
  "family": "railwiseTask",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",               
  "memory": "512",             
  "containerDefinitions": [
    {
      "name": "my-container",
      "image": "REPOSITORY_URI:IMAGE_TAG",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
